<shapes name="Flow">

	<shape name="StartNode" h="50" w="50" aspect="variable" strokewidth="inherit">
		<connections>
			<constraint x="1" y="0.5" perimeter="0" name="O" />
		</connections>
		<background>
			<path>
				<move x="0" y="0" />
				<line x="50" y="25" />
